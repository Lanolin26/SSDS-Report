<?xml version="1.0" encoding="utf-8"?>
<xmile version="1.0" xmlns="http://docs.oasis-open.org/xmile/ns/XMILE/v1.0">
  <header>
    <product version="1.0" lang="en">Vensim</product>
    <vendor>Ventana Systems, Inc.</vendor>
    <created>Kiselev Artem, Sherbakov Victor, Yaroshevsky Maksim</created>
    <modified>Kiselev Artem</modified>
    <name>Connect cells</name>
    <caption>Ð¡onnecting two cells into one</caption>
 </header>
  <sim_specs method="RK4" time_units="Hour">
    <start>0</start>
    <stop>100</stop>
    <dt>1</dt>
 </sim_specs>
  <model>
    <variables>
      <stock name="Car_Count_A">
        <units>cars</units>
        <doc></doc>
        <eqn>0</eqn>
        <inflow>in A</inflow>
        <outflow>inout1</outflow>
    </stock>
      <stock name="Car_Count_B">
        <units>cars</units>
        <doc></doc>
        <eqn>0</eqn>
        <inflow>in B</inflow>
        <outflow>inout2</outflow>
    </stock>
      <stock name="Car_Count_C">
        <units>cars</units>
        <doc></doc>
        <eqn>0</eqn>
        <inflow>inout1</inflow>
        <inflow>inout2</inflow>
        <outflow>out C</outflow>
    </stock>
      <aux name="C_A">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>C0_A * Ln_A</eqn>
    </aux>
      <aux name="C_B">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>C0_B * Ln_B</eqn>
    </aux>
      <aux name="C_C">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>C0_C * Ln_C</eqn>
    </aux>
      <aux name="coef_B">
        <units></units>
        <doc></doc>
        <eqn>1 - coef_A</eqn>
    </aux>
      <aux name="F1">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>IF_THEN_ELSE( s_A + s_B <= r_C , s_A ,(min(s_A , min (r_C - s_B , coef_A * r_C)) + max(s_A , max(r_C - s_B , coef_A * r_C)))/2)</eqn>
    </aux>
      <aux name="F2">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>IF_THEN_ELSE( s_A + s_B <= r_C , s_B ,(min(s_B , min (r_C - s_A , coef_B * r_C)) + max(s_B , max(r_C - s_A , coef_B * r_C)))/2)
      </eqn>
    </aux>
      <aux name="IF">
        <units></units>
        <doc></doc>
        <eqn>IF_THEN_ELSE( s_A + s_B <= r_C , 0 , 1 )
      </eqn>
    </aux>
      <aux name="in_A">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>min(s_A_0, r_A)</eqn>
    </aux>
      <aux name="in_B">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>min(s_B_0, r_B)</eqn>
    </aux>
      <aux name="inout1">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>F1</eqn>
    </aux>
      <aux name="inout2">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>F2</eqn>
    </aux>
      <aux name="N_A">
        <units>cars</units>
        <doc></doc>
        <eqn>N0_A * Ln_A</eqn>
    </aux>
      <aux name="N_B">
        <units>cars</units>
        <doc></doc>
        <eqn>N0_B * Ln_B</eqn>
    </aux>
      <aux name="N_C">
        <units>cars</units>
        <doc></doc>
        <eqn>N0_C * Ln_C</eqn>
    </aux>
      <aux name="out_C">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>min(s_C, r_C_0)</eqn>
    </aux>
      <aux name="r_A">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>min( C_A, (V0_A / Speed_A) * ((N_A - Car_Count_A) / TIME_STEP) )</eqn>
    </aux>
      <aux name="r_B">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>min( C_B, (V0_B / Speed_B) * ((N_B - Car_Count_B) / TIME_STEP) )</eqn>
    </aux>
      <aux name="r_C">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>min( C_C, (V0_C / Speed_C) * ((N_C - Car_Count_C) / TIME_STEP) )</eqn>
    </aux>
      <aux name="s_A">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>min(C_A, Car_Count_A / TIME_STEP)</eqn>
    </aux>
      <aux name="s_B">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>min(C_B, Car_Count_B / TIME_STEP)</eqn>
    </aux>
      <aux name="s_C">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>min(C_C, Car_Count_C / TIME_STEP)</eqn>
    </aux>
      <aux name="C0_A">
        <units>cars/(Hour*km)</units>
        <doc></doc>
        <eqn>10</eqn>
    </aux>
      <aux name="C0_B">
        <units>cars/(Hour*km)</units>
        <doc></doc>
        <eqn>10</eqn>
    </aux>
      <aux name="C0_C">
        <units>cars/(Hour*km)</units>
        <doc></doc>
        <eqn>10</eqn>
    </aux>
      <aux name="coef_A">
        <units></units>
        <doc></doc>
        <eqn>1</eqn>
    </aux>
      <aux name="Ln_A">
        <units>km</units>
        <doc></doc>
        <eqn>2</eqn>
    </aux>
      <aux name="Ln_B">
        <units>km</units>
        <doc></doc>
        <eqn>2</eqn>
    </aux>
      <aux name="Ln_C">
        <units>km</units>
        <doc></doc>
        <eqn>2</eqn>
    </aux>
      <aux name="N0_A">
        <units>cars/km</units>
        <doc></doc>
        <eqn>10</eqn>
    </aux>
      <aux name="N0_B">
        <units>cars/km</units>
        <doc></doc>
        <eqn>10</eqn>
    </aux>
      <aux name="N0_C">
        <units>cars/km</units>
        <doc></doc>
        <eqn>10</eqn>
    </aux>
      <aux name="r_C_0">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>100</eqn>
    </aux>
      <aux name="s_A_0">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>100</eqn>
    </aux>
      <aux name="s_B_0">
        <units>cars/Hour</units>
        <doc></doc>
        <eqn>100</eqn>
    </aux>
      <aux name="Speed_A">
        <units>km/Hour</units>
        <doc></doc>
        <eqn>80</eqn>
    </aux>
      <aux name="Speed_B">
        <units>km/Hour</units>
        <doc></doc>
        <eqn>80</eqn>
    </aux>
      <aux name="Speed_C">
        <units>km/Hour</units>
        <doc></doc>
        <eqn>80</eqn>
     </aux>
      <aux name="V0_A">
        <units>km/Hour</units>
        <doc></doc>
        <eqn>60</eqn>
     </aux>
      <aux name="V0_B">
        <units>km/Hour</units>
        <doc></doc>
        <eqn>60</eqn>
     </aux>
      <aux name="V0_C">
        <units>km/Hour</units>
        <doc></doc>
        <eqn>60</eqn>
     </aux>
   </variables>
 </model>
</xmile>